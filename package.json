{
  "name": "projectname",
  "version": "1.0.1",
  "description": "description",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --open --config webpack/webpack.dev.js --progress",
    "build": "webpack --config webpack/webpack.prod.js --env production --progress",
    "test": "webpack serve --open --config webpack/webpack.dev.js --env production --progress",
    "release": "bash script/release.sh",
    "analyse": "webpack --config webpack/webpack.config.js --profile --json>stats.json",
    "analyze": "source-map-explorer dist/js/*.js",
    "lint": "eslint --quiet  --cache --ext .jsx,.js --fix ./src",
    "fix": "prettier --write  ./src",
    "precommit": "lint-staged",
    "init": "standard-version -- --first-release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@tsconfig/recommended": "^1.0.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "mockjs": "^1.1.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.7",
    "postcss-loader": "^5.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "source-map-explorer": "^2.5.2",
    "standard-version": "^9.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@jd/jmtd": "^1.19.0-alpha.0",
    "@jd/jmtd-charts": "^3.2.17-alpha.17",
    "@jd/jmtd-pro": "^1.18.0",
    "@jd/jmtd-tools": "^3.5.2-alpha.0",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --quiet  --cache --ext .jsx,.js --fix ./src",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        100
      ],
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "test",
          "revert",
          "perf",
          "chore",
          "conflict"
        ]
      ]
    }
  },
  "standard-version": {
    "changelogHeader": "# Change Log",
    "types": [
      {
        "type": "feat",
        "section": "需求列表"
      },
      {
        "type": "fix",
        "section": "Bug修复"
      },
      {
        "type": "perf",
        "section": "优化列表"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "conflict",
        "hidden": true
      },
      {
        "type": "revert",
        "hidden": true
      }
    ]
  }
}
